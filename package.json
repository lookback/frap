{
  "name": "frap",
  "version": "0.0.0",
  "description": "A tiny Functional Reactive architecture for JS apps.",
  "main": "build/index.js",
  "scripts": {
    "lint": "tslint --format stylish --project .",
    "pretest": "npm run lint",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "tsc && browserify build/src/index.js -s setup | terser -c -m > dist/frap.min.js",
    "test-bundle": "tsc && browserify build/examples/basic-react-typescript/react.js -d > test/bundle.js"
  },
  "author": "Martin Algesten <martin@lookback.io>",
  "contributors": [
    "Johan Brook <johan@lookback.io>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lookback/frap.git"
  },
  "bugs": {
    "url": "https://github.com/lookback/frap/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "browserify": "^16",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "terser": "^3",
    "tslint": "^5",
    "tslint-immutable": "^5",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "xstream": "^11.10.0"
  }
}
